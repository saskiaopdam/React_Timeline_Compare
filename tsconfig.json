{
  "compilerOptions": {
    "strictNullChecks": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "module": "esnext",
    "declaration": true,
    "outDir": "./lib",
    "noImplicitAny": true,
    "importHelpers": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  // "include": [
  //   "./node_modules/vertical-timeline-component-for-react/dist/index.d.ts",
  //   "./node_modules/@merc/react-timeline/dist/index.d.ts"
  // ],
  "compileOnSave": true
}
